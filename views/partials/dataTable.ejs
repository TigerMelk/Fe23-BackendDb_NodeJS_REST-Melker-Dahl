<% if (typeof dbData != 'undefined' && dbData.length > 0) { %>
<table>
  <thead>
    <tr>
      <th>First Name</th>
      <th>Last Name</th>
      <th>Town</th>
    </tr>
  </thead>
  <tbody>
    <% dbData.forEach(student => { %>
    <tr>
      <td><%= student.fName %></td>
      <td><%= student.lName %></td>
      <td><%= student.town %></td>
    </tr>
    <% }) %>
  </tbody>
</table>
<% } else { %>
<p>No students found.</p>
<% } %>
